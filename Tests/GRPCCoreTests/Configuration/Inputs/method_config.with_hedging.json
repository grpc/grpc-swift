{
  "name": [
    {
      "service": "echo.Echo",
      "method": "Get"
    }
  ],
  "waitForReady": true,
  "timeout": "1s",
  "maxRequestMessageBytes": 1024,
  "maxResponseMessageBytes": 2048,
  "hedgingPolicy": {
    "maxAttempts": 3,
    "hedgingDelay": "42s",
    "nonFatalStatusCodes": [
      "ABORTED",
      "UNIMPLEMENTED"
    ]
  }
}
